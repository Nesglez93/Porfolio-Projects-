ex="57" value="101"/><column index="58" value="90"/><column index="59" value="135"/><column index="60" value="170"/><column index="61" value="94"/><column index="62" value="163"/><column index="63" value="74"/><column index="64" value="225"/><column index="65" value="170"/><column index="66" value="102"/><column index="67" value="240"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="owid-covid-data_death" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_"><sort><column index="6" mode="0"/></sort><column_widths><column index="1" value="57"/><column index="2" value="59"/><column index="3" value="74"/><column index="4" value="73"/><column index="5" value="66"/><column index="6" value="86"/><column index="7" value="70"/><column index="8" value="133"/><column index="9" value="77"/><column index="10" value="76"/><column index="11" value="139"/><column index="12" value="141"/><column index="13" value="140"/><column index="14" value="203"/><column index="15" value="147"/><column index="16" value="146"/><column index="17" value="209"/><column index="18" value="109"/><column index="19" value="75"/><column index="20" value="145"/><column index="21" value="86"/><column index="22" value="156"/><column index="23" value="139"/><column index="24" value="209"/><column index="25" value="150"/><column index="26" value="220"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1">-- Looking at Total Cases Vs Total Deaths  In Mexico 
-- Shows percentage of Deaths  in the Country 

SELECT
	location,
	date,
	population,
	total_cases,
	total_deaths,
	(CAST(total_deaths AS REAL)/total_cases)*100 as DeathPercentage	
FROM
	CovidDeath
WHERE 
location like'%mexico%' 


------ Looking at the countries with highest Infection rate compared to the Population

SELECT
	location, 
	date,
	population,
	MAX(total_cases) as HighestInfectionCount, 
	(MAX(total_cases *1.0)/population)*100 as InfectedPopulationPercentage	
FROM
	CovidDeath
GROUP BY 
	location,
	population
	
ORDER BY 
	InfectedPopulationPercentage DESC
	
--- Looking at countries with the highest Death counts  

SELECT
	location, 
	population,
	MAX(total_deaths) as HighestDeathCount, 
	(MAX(total_deaths *1.0)/population)*100 as DeathPopulationPercentage	
FROM
	CovidDeath
WHERE
	continent IS NOT NULL
GROUP BY 
	location,
	population
	
ORDER BY 
	HighestDeathCount DESC
	
-- Breaking the Data down by continent 

SELECT
	continent,
	MAX(total_deaths) as HighestDeathCount 
FROM
	CovidDeath
WHERE
	continent IS NOT NULL 
GROUP BY 
	continent	
ORDER BY 
	HighestDeathCount DESC

--  GLOBAL NUMBERS 
SELECT
	SUM(new_cases) as total_casesw, 
	SUM(new_deaths) as total_deathsw,  
	SUM(new_deaths *1.0)/SUM(new_cases)*100 as DeathPercentage	
FROM
	CovidDeath
WHERE
	continent IS NOT NULL
ORDER BY 
1,2


----- Joining Tables 

SELECT * 
FROM 
	CovidDeath dea 
JOIN
	CovidVac vac 
ON dea.location = vac.location
and dea.date = vac.date 
	
	

--- Looking at Total Populatio VS Vaccinations 

	
SELECT 
	dea.continent, 
	dea.location, 
	dea.date,
	dea.population, 
	vac.new_vaccinations,
	SUM(vac.new_vaccinations) OVER (PARTITION BY dea.location ORDER BY dea.location, dea.date) AS PeopleVac
FROM 
	CovidDeath dea 
JOIN
	CovidVac vac 
ON dea.location = vac.location
and dea.date = vac.date 
WHERE
	dea.continent IS NOT NULL 
	
ORDER BY 
		1,2,3
		
---- Creating a CTE 
WITH PopvsVac (continent, location, date, population,new_vaccinations, PeopleVac)
as 
(
SELECT 
	dea.continent, 
	dea.location, 
	dea.date,
	dea.population, 
	vac.new_vaccinations,
	SUM(vac.new_vaccinations) OVER (PARTITION BY dea.location ORDER BY dea.location, dea.date) AS PeopleVac
FROM 
	CovidDeath dea 
JOIN
	CovidVac vac 
ON dea.location = vac.location
and dea.date = vac.date 
WHERE
	dea.continent IS NOT NULL 
	
--ORDER BY 
		--1,2,3
)
SELECT * ,(PeopleVac * 1.0 / population)*100 as vacper
FROM 
	PopvsVac
	
--- Creating Views  to store Data for later Visualizations  

CREATE VIEW PercentagePopulationVaccinated as

SELECT
	location, 
	date,
	population,
	MAX(total_cases) as HighestInfectionCount, 
	(MAX(total_cases *1.0)/population)*100 as InfectedPopulationPercentage	
FROM
	CovidDeath
GROUP BY 
	location,
	population
	
ORDER BY 
	InfectedPopulationPercentage DESC
